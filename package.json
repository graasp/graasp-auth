{
  "name": "graasp-auth",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@graasp/query-client": "github:graasp/graasp-query-client#155/invite",
    "@material-ui/core": "4.12.4",
    "@material-ui/icons": "4.11.3",
    "history": "5.3.0",
    "http-status-codes": "2.2.0",
    "i18next": "21.6.16",
    "prop-types": "15.8.1",
    "qs": "6.10.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-i18next": "11.16.7",
    "react-router": "6.3.0",
    "react-router-dom": "6.3.0",
    "react-scripts": "4.0.3",
    "react-toastify": "9.0.0",
    "validator": "13.7.0"
  },
  "scripts": {
    "start": "env-cmd -f ./.env.local react-scripts start",
    "start:test": "env-cmd -f ./.env.test react-scripts start",
    "start:ci": "react-scripts -r @cypress/instrument-cra start",
    "build": "react-scripts build",
    "dist:dev": "env-cmd -f ./.env.development react-scripts build",
    "dist": "env-cmd -f ./.env.production react-scripts build",
    "test": "env-cmd -f ./.env.test react-scripts test",
    "test:once": "env-cmd -f ./.env.test react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "pre-commit": "yarn prettier:check && yarn lint",
    "prettier:check": "prettier --check src/**/*.js",
    "prettier:write": "prettier --write src/**/*.js",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "cypress:open": "env-cmd -f ./.env.test cypress open",
    "cypress": "concurrently \"yarn start:test\" \"wait-on http://localhost:3001 && yarn cypress:run\"",
    "cypress:run": "env-cmd -f ./.env.test cypress run --headless --browser chrome"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.17.0",
    "@babel/preset-react": "7.16.7",
    "@commitlint/cli": "16.2.4",
    "@commitlint/config-conventional": "16.2.4",
    "@cypress/code-coverage": "3.9.12",
    "@cypress/instrument-cra": "1.4.0",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.2.0",
    "@testing-library/user-event": "14.1.1",
    "concurrently": "7.1.0",
    "cypress": "9.6.0",
    "env-cmd": "10.1.0",
    "eslint": "7.11.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "7.1.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.22.0",
    "husky": "7.0.4",
    "istanbul-lib-coverage": "3.2.0",
    "nyc": "15.1.0",
    "prettier": "2.6.2",
    "standard-version": "9.3.2",
    "typescript": "4.6.4",
    "wait-on": "6.0.1"
  },
  "nyc": {
    "exclude": [
      "src/serviceWorker.js"
    ]
  },
  "packageManager": "yarn@3.2.0"
}
